<Window x:Class="TeslaRevitTools.EquipmentTagQuality.NewParameterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeslaRevitTools.EquipmentTagQuality"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStyle="None"
        WindowStartupLocation="CenterOwner"
        Title="NewParameterWindow" Height="80" Width="240">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Border Grid.RowSpan="2" BorderBrush="Black" BorderThickness="1"></Border>
        <TextBox x:Name="ParameterName" Text="" Margin="5"></TextBox>
        <Button Content="Cancel" Grid.Row="2" HorizontalAlignment="Left" Margin="5,5,0,5" Width="70" Click="BtnCancel"></Button>
        <Button Content="Add new parameter" Padding="5,0,5,0" Grid.Row="2" HorizontalAlignment="Left" Margin="90,5,0,5" Click="BtnApply" Width="135"></Button>
    </Grid>
</Window>
